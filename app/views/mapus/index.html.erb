<h1>Listing mapus</h1>

<table>
  <thead>
    <tr>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Address</th>
      <th>Description</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mapus.each do |mapu| %>
      <tr>
        <td><%= mapu.longitude %></td>
        <td><%= mapu.latitude %></td>
        <td><%= mapu.address %></td>
        <td><%= mapu.description %></td>
        <td><%= mapu.title %></td>
        <td><%= link_to 'Show', mapu %></td>
        <td><%= link_to 'Edit', edit_mapu_path(mapu) %></td>
        <td><%= link_to 'Destroy', mapu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mapu', new_mapu_path %>
