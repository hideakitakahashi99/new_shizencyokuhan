<%= form_for @project do |f| %>

<p>
	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :address %>
	<%= f.text_field :address %>

	<%= f.label :description %>
	<%= f.text_field :description %>



	<% if @project.errors.any? %>
	<%= @project.errors.messages[:title][0] %>
	<% end %>
</p>

<p>
	<%= f.submit %>
</p>

<% end %>